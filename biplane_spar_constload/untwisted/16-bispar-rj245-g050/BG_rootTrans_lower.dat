@EDGE_DEFINITION {
  @EDGE_NAME {edgeBG} {
    @CONNECTED_TO_VERTICES {vertexB, vertexG}
    @CURVE_NAME {curveBG}
  }
}

@BEAM_DEFINITION {
  @BEAM_NAME {BG_rootTrans_lower} {
    @EDGE_NAME {edgeBG}
    @BEAM_PROPERTY_NAME {propBG}
    @SHAPE_NAME {shape_rootTrans}
    @GRAPHICAL_PARAMETERS_NAME {gp_biplaneLower}
    @COMMENTS {the lower root transition region of the spar}
  }
}

@INCLUDE_COMMAND {
  @INCLUDE_COMMAND_NAME {includePropsBG} {
    @ACTIVE_COMMAND {YES}
    @LIST_OF_FILE_NAMES {BG_rootTrans_lower_curve.dat, BG_rootTrans_lower_mesh.dat, BG_rootTrans_lower_props.dat}
  }
}

@SURVEY_DEFINITION {
  @SURVEY_NAME {surveyBG} {
    @OBJECT_NAME {BG_rootTrans_lower}
    @SURVEY_TYPE {DISPLACEMENTS}
    @FRAME_NAME {INERTIAL}
    @TIME {5.00000e+000}
  }
}

